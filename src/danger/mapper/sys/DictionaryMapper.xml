        name = #{record.name,jdbcType=VARCHAR},
      </if>
      <if test="record.updictionaryid != null" >
        upDictionaryId = #{record.updictionaryid,jdbcType=VARCHAR},
      </if>
      <if test="record.isuse != null" >
        isUse = #{record.isuse,jdbcType=VARCHAR},
      </if>
