  </update>
  <update id="updateByPrimaryKey" parameterType="danger.bean.sys.Dictionary" >
    update dictionary
    set name = #{name,jdbcType=VARCHAR},
      upDictionaryId = #{updictionaryid,jdbcType=VARCHAR},
